    "globals": {
      "__APP_CONFIG__": {
        "viceroyAPI": "http://viceroyAPI.edu",
        "recommendAPI": "http://recommendAPI.edu",
        "coursesAPI": "http://coursesAPI.edu",
        "resourcesAPI": "http://resourcesAPI.edu",
